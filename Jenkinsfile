  agent any
  stages {
    stage ('build'){
      steps {
        echo "Build step" 
        sleep 4
     }
   
    }
     stage ('test'){
      steps {
        echo "test stage" 
        sleel 5
     }
   
    }
     stage ('deploy'){
      steps {
        echo "deploy war file" 
        sleep 10
     }
   
    }
 }
}
